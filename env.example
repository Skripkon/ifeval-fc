# IFEval-FC Inference System Environment Configuration
# Copy this file to .env and fill in your API keys and configuration

# =============================================================================
# OpenAI Configuration
# =============================================================================
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_TEMPERATURE=0.0
OPENAI_MAX_TOKENS=1000
OPENAI_BASE_URL=https://api.openai.com/v1

# =============================================================================
# Anthropic Configuration
# =============================================================================
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_TEMPERATURE=0.0
ANTHROPIC_MAX_TOKENS=1000

# =============================================================================
# Google Configuration
# =============================================================================
GOOGLE_GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_MODEL=gemini-pro
GOOGLE_TEMPERATURE=0.0
GOOGLE_MAX_TOKENS=1000

# =============================================================================
# GigaChat Configuration
# =============================================================================
GIGACHAT_USER=YOUR_USER
GIGACHAT_PASSWORD=YOUR_PASSWORD
GIGACHAT_BASE_URL=YOUR_BASE_URL
GIGACHAT_MODEL=GigaChat-2-Max
GIGACHAT_TEMPERATURE=0.0
GIGACHAT_VERIFY_SSL_CERTS=False
GIGACHAT_PROFANITY_CHECK=False
GIGACHAT_REPETITION_PENALTY=1.0

# =============================================================================
# Inference System Configuration
# =============================================================================
DEFAULT_LLM_PROVIDER=openai
DEFAULT_MODEL=gpt-3.5-turbo
DEFAULT_TEMPERATURE=0.0
DEFAULT_MAX_TOKENS=1000

# =============================================================================
# Data Configuration
# =============================================================================
DATA_DIR=data
MAX_SAMPLES=null
VERBOSE=false
